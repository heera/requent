{
    "name": "sheikhheera/requent",
    "license": "MIT",
    "description": "A GraphQL like interface to map a request to eloquent query with data transformation.",
    "keywords": ["requent", "api", "json", "transformer", "gql", "graph", "query", "laravel"],
    "authors": [
        {
            "name": "Sheikh Heera",
            "email": "heera.sheikh77@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/http": ">=5.3",
        "illuminate/config": ">=5.3",
        "illuminate/database": ">=5.3",
        "illuminate/pagination": ">=5.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Requent\\": "src/Requent"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Requent\\": "tests/Requent/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Requent\\RequentServiceProvider"
            ],
            "aliases": {
                "Requent": "Requent\\Facade\\Requent"
            }
        }
    }
}
